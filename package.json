{
  "name": "director-web-player",
  "version": "0.1.0",
  "type": "module",
  "description": "Client-side Director player using AssemblyScript/WebAssembly",
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts -o build/director.wasm --sourceMap --debug",
    "asbuild:release": "asc assembly/index.ts -o build/director.wasm -O3 --noAssert",
    "build": "npm run asbuild:release",
    "dev": "npm run asbuild:debug",
    "test": "node --experimental-wasm-modules tests/run.js"
  },
  "devDependencies": {
    "assemblyscript": "^0.27.0",
    "@assemblyscript/loader": "^0.27.0"
  },
  "license": "MPL-2.0"
}
